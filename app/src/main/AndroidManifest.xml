<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.fastspeed.five5gbrowser">

    <uses-permission android:name="android.permission.INTERNET" />
    <!--    <uses-permission android:name="android.permission.FOREGROUND_SERVICE" />-->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name="com.fastspeed.five5gbrowser.mynotification_bholu.BholuApp"
        android:allowBackup="false"
        android:hardwareAccelerated="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        tools:replace="android:allowBackup"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="com.fastspeed.five5gbrowser.rld_bholu.core_bholu.BholuSplashActivity"
            android:configChanges="uiMode"
            android:resizeableActivity="false"
            android:screenOrientation="portrait"
            tools:ignore="LockedOrientationActivity,NonResizeableActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.fastspeed.five5gbrowser.browser_bholu.BholuHistoryActivity"
            android:configChanges="uiMode"
            android:resizeableActivity="false"
            tools:ignore="LockedOrientationActivity,NonResizeableActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.fastspeed.five5gbrowser.browser_bholu.BholuBookamrksActivity"
            android:configChanges="uiMode"
            android:resizeableActivity="false"
            tools:ignore="LockedOrientationActivity,NonResizeableActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.fastspeed.five5gbrowser.browser_bholu.BholuWebActivity"
            android:configChanges="uiMode"
            android:resizeableActivity="false"
            tools:ignore="LockedOrientationActivity,NonResizeableActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateAlwaysHidden" />
        <activity
            android:name="com.fastspeed.five5gbrowser.BholuStartActivity"
            android:exported="true"
            tools:ignore="LockedOrientationActivity,NonResizeableActivity"
            android:configChanges="uiMode"
            android:resizeableActivity="false"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.fastspeed.five5gbrowser.rld_bholu.core_bholu.BholuWerAppUpdateActivity"
            android:configChanges="uiMode"
            android:resizeableActivity="false"
            tools:ignore="LockedOrientationActivity,NonResizeableActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.fastspeed.five5gbrowser.rld_bholu.core_bholu.BholuMaintenanceActivity"
            android:configChanges="uiMode"
            android:resizeableActivity="false"
            tools:ignore="LockedOrientationActivity,NonResizeableActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.fastspeed.five5gbrowser.rld_bholu.core_bholu.BholuWeMovedActivity"
            android:configChanges="uiMode"
            tools:ignore="LockedOrientationActivity,NonResizeableActivity"
            android:resizeableActivity="false"
            android:screenOrientation="portrait" />

        <meta-data
            android:name="com.google.android.gms.ads.AD_MANAGER_APP"
            android:value="true" />



    </application>
</manifest>